{% extends 'backend/base.html.twig' %}
{% block nav %}
    {% include 'backend/include/navbar.html.twig' %}
{% endblock %}
{% block sidebar %}
    {% include 'backend/include/sidebar.html.twig' %}
{% endblock %}


{% block title %}Participant{% endblock %}

{% block body %}
<div class="content-page">
    <div class="container-fluid">

    <h1>Participant</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ participant.id }}</td>
        </tr>
        <tr>
            <th>Id_user</th>
            <td>{{ participant.user }}</td>
        </tr>

        <tr>
            <th>Date</th>
            <td>{{ participant.date ? participant.date|date('Y-m-d') : '' }}</td>
        </tr>


        </tbody>
    </table>

    <a href="{{ path('participant_index_backend') }}">back to list</a>

    <a href="{{ path('participant_edit_backend', {'id': participant.id}) }}">edit</a>

    {{ include('backend/participant/_delete_form.html.twig') }}
    </div>
</div>
{% endblock %}
